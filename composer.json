{
    "name": "tannhatcms/theme-tabler-lms",
    "description": "UI for Backpack v6 that uses Tabler and Bootstrap v5 for LMS CMS.",
    "license": "MIT",
    "homepage": "https://github.com/tannhatcms/theme-tabler-lms",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "ThemeTabler"
    ],
    "authors": [
        {
            "name": "TanNhatCMS",
            "email": "mrkiet.dev@gmail.com",
            "homepage": "https://mrkiet.name.vn"
        }
    ],
    "require": {
        "tannhatcms/crud-lms": "^6.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "TanNhatCMS\\ThemeTablerLMS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TanNhatCMS\\ThemeTablerLMS\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "TanNhatCMS\\ThemeTablerLMS\\AddonServiceProvider"
            ]
        }
    }
}
